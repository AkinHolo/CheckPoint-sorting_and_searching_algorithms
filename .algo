PROCEDURE insertion_sort(tab: ARR_OF INTEGER, elt: INTEGER): INTEGER

VAR
i,j,n: INTEGER;
swap: tab[i];

BEGIN
    n:= tab.length
    FOR i FROM 1 TO n-1 DO
        j = i
        WHILE j > 0 AND tab[j-1] > tab[j] DO
            swap(tab[j], tab[j-1])
            j = j - 1
        END_WHILE
    END_FOR

WRITE(tab, n);


